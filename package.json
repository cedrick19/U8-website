{
  "name": "gamingwebsite",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "gamingwebsite",
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run dev && npm run prepare",
    "prod": "npm run build && npx serve www --single",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build && npx workbox generateSW workbox-config.cjs",
    "lint": "eslint .",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*{json,css,md}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "IOS >= 15",
    "Safari >= 15",
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "dependencies": {
    "@tailwindcss/postcss": "^4.0.15",
    "clsx": "^2.1.1",
    "dom7": "^4.0.6",
    "framework7": "^8.3.4",
    "framework7-icons": "^5.0.5",
    "framework7-react": "^8.3.4",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "material-icons": "^1.13.14",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "skeleton-elements": "^4.0.1",
    "swiper": "^11.2.6",
    "tailwind-merge": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/node": "^22.13.11",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.23.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "postcss": "^8.5.3",
    "postcss-preset-env": "^10.1.5",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sass": "^1.86.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vite": "^6.2.2",
    "workbox-cli": "^7.3.0"
  },
  "framework7": {
    "noInstall": true
  }
}
